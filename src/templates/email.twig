<!-- Checkboxes -->
<div class="block">
    <table width="100%" bgcolor="#f5f8fa" cellpadding="0" cellspacing="0" border="0" id="backgroundTable"
           st-sortable="fullimage" style="min-width:100%;">
        <tbody>
        <tr>
            <td><!-- ATTENTION: on FIRST element set the border-top radius-->
                <table bgcolor="#ffffff" width="580" align="center" cellspacing="0" cellpadding="0" border="0"
                       class="devicewidth" modulebg="edit"
                       style="border-top: 1px solid #e8e8e8; border-left: 1px solid #e8e8e8; border-right: 1px solid #e8e8e8; overflow: hidden; border-collapse: separate; border-top-left-radius:3px; border-top-right-radius: 3px;">
                    <tbody>
                    <tr>
                        <td width="100%" height="20"></td>
                    </tr>
                    <tr>
                        <td>
                            <table width="540" align="center" cellspacing="0" cellpadding="0" border="0"
                                   class="devicewidthinner">
                                <tbody>
                                <!-- Avatar with img and text chat -->
                                <tr>
                                    <td>
                                        <table width="540" align="center" cellspacing="0" cellpadding="0" border="0"
                                               class="devicewidthinner">
                                            <tbody>
                                            <!-- title -->
                                            <tr>
                                                <td style="font-family: Helvetica, Arial, sans-serif; color: #999999; text-align: left; font-size: 11px; line-height: 16px; font-weight: bold;"
                                                    st-title="rightimage-title">
                                                    Neue Kommentare in <a
                                                            href="https://projects.kreisvier.ch/#/groups/{{settings.groupId}}"
                                                            style="font-family: Helvetica, Arial, sans-serif; color: #57a4f9; font-size: 11px; line-height: 16px; font-weight: bold;"
                                                            target="_blank">{{ settings.groupName }}</a></td>
                                            </tr>
                                            {% if meeps.success == true %}
                                                {% for meep in meeps.result %}
                                                    <!-- Spacing -->
                                                    <tr>
                                                        <td width="100%" height="13"></td>
                                                    </tr>
                                                    <!-- Text -->
                                                    <tr>
                                                        <td>
                                                            <table border="0" cellpadding="0" cellspacing="0">
                                                                <tbody>
                                                                <tr>
                                                                    <!-- Avatar, with a hiding for Outlook hack for the ones which do not support position: relative -->
                                                                    <td valign="top"><a target="_blank"
                                                                                        href="https://projects.kreisvier.ch/#/users/{{ meep.user.id }}"><img
                                                                                    width="40" height="40"
                                                                                    src="{{ meep.user.hex_avatar }}"
                                                                                    alt="{{ meep.user.first_name }} {{ meep.user.last_name }}"
                                                                                    border="0"
                                                                                    style="display: block;">
                                                                        </a></td>
                                                                    <!-- Content -->
                                                                    <td valign="top">
                                                                        <table border="0" cellpadding="0" cellspacing="0">
                                                                            <tbody>
                                                                            <tr>
                                                                                <!-- Less padding bottom when text is following -->
                                                                                <td style="padding-left: 15px; padding-bottom: 5px; font-family: Helvetica, Arial, sans-serif; font-size: 14px; line-height: 14px; font-weight: bold; color: #1a1a1a;">
                                                                                    {{ meep.user.first_name }} {{ meep.user.last_name }}
                                                                                    <span class="appleLinks"
                                                                                          style="font-family: Helvetica, Arial, sans-serif; font-size: 10px; line-height: 14px; font-weight: normal; color: #999999;">{{ meep.created_at|date("d.m H:i") }}</span>
                                                                                </td>
                                                                            </tr>
                                                                            <!-- Text -->
                                                                            <tr>
                                                                                <td style="padding-left: 15px; font-family: Helvetica, Arial, sans-serif; font-size: 13px; line-height: 18px; color: #1a1a1a;">
                                                                                    <p>{{ meep.message|raw }}</p>
                                                                                </td>
                                                                            </tr>
                                                                            {% if meep.files|length %}
                                                                                <!-- Images -->
                                                                                <tr>
                                                                                    <td style="padding-left: 15px;">
                                                                                        {% for file in meep.files %}

                                                                                            <!-- INDD file ( ATTENTION: when custom file we need "padding-right: 5px" in parent for Outlook ) -->
                                                                                            <div style="float: left; margin-right: 5px; margin-bottom: 5px;">
                                                                                                <table cellspacing="0"
                                                                                                       cellpadding="0"
                                                                                                       border="0"
                                                                                                       bgcolor="#71c084"
                                                                                                       style="float: left;">
                                                                                                    <tbody>
                                                                                                    <tr>
                                                                                                        <td width="100%"
                                                                                                            height="20"
                                                                                                            style="height: 20px;">
                                                                                                            <a target="_blank"
                                                                                                               href="https://projects.kreisvier.ch/#/finder_large_preview?meep_id={{ meep.id }}&meep_file_id={{ file.id }}"
                                                                                                               style="display: block; width: 60px; height: 20px; font-size: 5px; line-height: 5px; text-decoration: none;">&nbsp;</a>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <td width="60"
                                                                                                            height="20"
                                                                                                            style="height: 20px; width: 60px; text-align: center;">
                                                                                                            <a target="_blank"
                                                                                                               href="https://projects.kreisvier.ch/#/finder_large_preview?meep_id={{ meep.id }}&meep_file_id={{ file.id }}"
                                                                                                               style="display: block; width: 60px; height: 20px; text-transform: uppercase; text-decoration: none; color: #FFFFFF; font-family: Helvetica, Arial, sans-serif; font-weight: lighter; font-size: 14px; line-height: 16px;">
                                                                                                                {{ file.name|split('.')|reverse[0] }}
                                                                                                            </a></td>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <td width="100%"
                                                                                                            height="20"><a
                                                                                                                    target="_blank"
                                                                                                                    href="https://projects.kreisvier.ch/#/finder_large_preview?meep_id={{ meep.id }}&meep_file_id={{ file.id }}"
                                                                                                                    style="display: block; width: 60px; height: 20px; font-size: 5px; line-height: 5px; text-decoration: none;">&nbsp;</a>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                    </tbody>
                                                                                                </table>
                                                                                            </div>
                                                                                            <!--[if mso]><span style="font-size: 5px; line-height: 5px;">&nbsp;</span><![endif]-->
                                                                                            <!-- space for Outlook -->

                                                                                        {% endfor %}
                                                                                    </td>
                                                                                </tr>
                                                                            {% endif %}
                                                                            <!-- Spacing -->
                                                                            <tr>
                                                                                <td width="100%" height="8"></td>
                                                                            </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </td>
                                                                </tr>
                                                                </tbody>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                {% endfor %}
                                            {% else %}
                                                <tr>
                                                    <td style="font-family: Helvetica, Arial, sans-serif; color: #999999; text-align: left; font-size: 11px; line-height: 16px; font-weight: bold;"
                                                        st-title="rightimage-title">
                                                        {{ meeps.result }}

                                                    </td>
                                                </tr>
                                            {% endif %}
                                            <tr>
                                                <td width="100%" height="20"></td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </td>
        </tr>
        </tbody>
    </table>
